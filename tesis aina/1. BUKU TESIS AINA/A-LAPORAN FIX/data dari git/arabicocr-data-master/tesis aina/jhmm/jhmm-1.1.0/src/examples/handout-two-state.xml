<hmm xmlns="urn:publicid:IDN+milowski.com:schemas:math:hmm:2004:us" name="Handout - Two State/Binary" error="0.0005">

<description>
<body xmlns="http://www.w3.org/1999/xhtml">
<p>This is the example from the class handout.</p>
</body>
</description>

<alphabet lexicon="01"/>

<start at="s0" probability="0.5"/>
<start at="s1" probability="0.5"/>

<state id="s0">
<transition to="s0" probability="0.75"/>
<transition to="s1" probability="0.25"/>
<emissions>
<symbol char="0" probability="0.6666"/>
<symbol char="1" probability="0.3333"/>
</emissions>
</state>

<state id="s1">
<transition to="s0" probability="0.25"/>
<transition to="s1" probability="0.75"/>
<emissions>
<symbol char="0" probability="0.3333"/>
<symbol char="1" probability="0.6666"/>
</emissions>
</state>

<final at="s0" probability="0.5"/>
<final at="s1" probability="0.5"/>

</hmm>
